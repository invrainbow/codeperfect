{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "debug",
            "program": "${workspaceFolder}/build/bin/ide",
            "args": [
                "__debug__",
            ],
            "cwd": "${workspaceFolder}/build/bin",
            "expressions": "native",
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "test",
            "program": "build/bin/test",
            "args": [
                "parser_profile"
            ],
            "cwd": "${workspaceFolder}/build/bin",
            "expressions": "native"
        },
        {
            "type": "lldb",
            "request": "custom",
            "name": "coredump",
            "initCommands": [
                "target create -c ${input:coreFileName}"
            ],
            "expressions": "native"
        },
        {
            "type": "lldb",
            "request": "attach",
            "name": "attach",
            "program": "build/bin/ide",
            "expressions": "native"
        },
    ],
    "inputs": [
        {
            "id": "coreFileName",
            "type": "promptString",
            "description": "Corefile path"
        }
    ],
}